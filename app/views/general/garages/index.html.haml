- title t('my_objects_garages')
%h1.heading= t('my_objects_garages')

.actions-bar
  %div{:class => "left search"}
    - form_for @search do |f|
      = f.text_field :address_like, :class => 'input-s'
      = f.submit t("filter_button")
  .right
    = will_paginate @collection
  .clear
  
%p.actions
  = link_to t('new_garage_link'), new_resource_path
  
%table
  %thead
    %tr
      %th.title= t('activerecord.attributes.garage.title')
      %th.address= t('activerecord.attributes.garage.address')
      %th.square= t('activerecord.attributes.garage.square')
      %th.price= t('activerecord.attributes.garage.price')
      %th.actions= t('actions')
  %tbody.items.garages
    - @garages.each do |garage|
      = render 'item', :garage => garage

= will_paginate @collection
 
